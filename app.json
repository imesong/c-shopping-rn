{
  "expo": {
    "name": "c-shop",
    "slug": "c-shop",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "experiments": {
      "tsconfigPaths": true
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.huanghanzhilian.cshoppingrn"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.huanghanzhilian.cshoppingrn"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "shoppig-rn-scheme",
    "plugins": ["expo-router"],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "289f8df7-367d-4cc7-89f5-df37984df7ab"
      }
    }
  }
}
